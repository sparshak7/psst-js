{
  "name": "psst-js",
  "version": "1.0.3",
  "description": "A lightweight JavaScript browser fingerprinting utility with UMD and ESM support.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./script": "./dist/psst-collector.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:umd": "rollup -c"
  },
  "keywords": [
    "fingerprint",
    "browser-fingerprint",
    "analytics",
    "tracking",
    "hashing",
    "UMD",
    "ESM",
    "javascript-library"
  ],
  "author": "Sparshak Nag <sparshaknag247@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparshak7/psst-js.git"
  },
  "bugs": {
    "url": "https://github.com/sparshak7/psst-js/issues"
  },
  "homepage": "https://github.com/sparshak7/psst-js#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/murmurhash-js": "^1.0.6",
    "@types/node": "^24.0.7",
    "rollup": "^4.44.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
